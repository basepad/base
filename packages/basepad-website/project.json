{
  "name": "basepad-website",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/basepad-website/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/basepad-website",
        "postcssConfig": "packages/basepad-website/postcss.config.js"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "basepad-website:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "basepad-website:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "basepad-website:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "basepad-website:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "basepad-website:build:development"
        },
        "production": {
          "buildTarget": "basepad-website:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/basepad-website"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/packages/basepad-website"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/basepad-website/**/*.{ts,tsx,js,jsx}"],
        "fix": true
      }
    }
  },
  "tags": []
}
